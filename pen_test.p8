function _draw()
 cls()
 for y,line in pairs(
  split(str,'\n',false)
 ) do
  local x=0
  for i=1,#line,2 do
   local p,len=ord(line,i,2)
   p-=0x30
   len-=0x30
   rectfill(x,y,x+len,y,p)
   x+=len
  end
 end
end
